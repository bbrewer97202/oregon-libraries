{
  "name": "oregon-libraries",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "type-check": "tsc",
    "generate-data": "ts-node scripts/generate-data-from-api-json.ts",
    "generate-data:example": "ts-node scripts/generate-data-from-api-json.ts --example=true",
    "generate-data-client": "npx prisma generate",
    "db:create": "npm run db:remove && npm run generate-data-client && npx prisma migrate dev --name init",
    "db:seed": "npx prisma db seed",
    "db:remove": "rimraf data/migrations && rimraf data/data.db && rimraf data/data.db-journal",
    "initialize": "npm run generate-data && npm run db:create && npm run generate-data-client && npm run db:seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@types/got": "^9.6.12",
    "got": "^11.8.3",
    "leaflet": "^1.7.1",
    "next": "^12.0.7",
    "parse-address": "^1.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-leaflet": "^3.2.2",
    "slugify": "^1.6.4"
  },
  "devDependencies": {
    "@prisma/client": "^3.7.0",
    "@types/leaflet": "^1.7.7",
    "@types/node": "^17.0.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-leaflet": "^2.8.2",
    "@types/shortid": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1",
    "prisma": "^3.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "license": "MIT"
}
